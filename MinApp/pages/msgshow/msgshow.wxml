<view class="container">
    <view class = "item">
        昵称：{{nickname}}
    </view>
    <view class = "item">
        姓名： {{name}}
    </view>
    <view class = "item" wx:if="{{gender==1}}">
        性别：女
    </view>
    <view class = "item" wx:else>
        性别：男
    </view>
    <view class = "item">
        年龄：{{age}}
    </view>
    <view class = "item">
        电话号码：{{phone}}
    </view>
    <view class = "item">
        <button bindtap="toUsermsg">修改信息</button>
    </view>

</view>

